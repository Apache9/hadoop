<?xml version="1.0" encoding="UTF-8"?><Configuration name="Log4j1">
  <Appenders>
    <Console name="kms-audit" target="SYSTEM_ERR">
      <PatternLayout pattern="%m"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="kms-audit"/>
    </Root>
    <Logger name="org.apache.hadoop.crypto.key.kms.server.KMSAudit" level="OFF"/>
  </Loggers>
</Configuration>
